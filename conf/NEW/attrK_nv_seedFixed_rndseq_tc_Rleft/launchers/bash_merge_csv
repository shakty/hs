#!/bin/sh
OUTFILE_PARAMS="/cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/params_all.csv"
OUTFILE_MACRO="/cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/clusters_macro_all.csv"
OUTFILE_MICRO="/cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/clusters_micro_all.csv"
OUTFILE_MACRO_AVG_SPLIT="/cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/clusters_macro_avg_split.csv"
