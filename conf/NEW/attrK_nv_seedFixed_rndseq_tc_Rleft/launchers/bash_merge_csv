sed -e '1d' /cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/attrK_nv_seedFixed_rndseq_tc_Rleft_s3/clusters_macro.csv >> $OUTFILE_MACRO
sed -e '1d' /cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/attrK_nv_seedFixed_rndseq_tc_Rleft_s3/clusters_micro.csv >> $OUTFILE_MICRO
