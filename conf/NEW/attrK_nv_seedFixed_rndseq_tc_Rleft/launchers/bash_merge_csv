sed -e '1d' /cluster/work/scr1/balistef/NEW/attrK_nv_seedFixed_rndseq_tc_Rleft/attrK_nv_seedFixed_rndseq_tc_Rleft_s2/params.csv >> $OUTFILE_PARAMS
