sed -e '1d' /cluster/work/scr2/balistef/NEW/attrLinear_nv_Kseed_rndseq_tm_Rleft_n200/attrLinear_nv_Kseed_rndseq_tm_Rleft_n200_s5/params.csv >> $OUTFILE_PARAMS
sed -e '1d' /cluster/work/scr2/balistef/NEW/attrLinear_nv_Kseed_rndseq_tm_Rleft_n200/attrLinear_nv_Kseed_rndseq_tm_Rleft_n200_s5/clusters_macro.csv >> $OUTFILE_MACRO
