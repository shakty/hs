sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s2/params.csv >> $OUTFILE_PARAMS
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s2/clusters_macro.csv >> $OUTFILE_MACRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s2/clusters_micro.csv >> $OUTFILE_MICRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s3/params.csv >> $OUTFILE_PARAMS
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s3/clusters_macro.csv >> $OUTFILE_MACRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s3/clusters_micro.csv >> $OUTFILE_MICRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s4/params.csv >> $OUTFILE_PARAMS
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s4/clusters_macro.csv >> $OUTFILE_MACRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s4/clusters_micro.csv >> $OUTFILE_MICRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s5/params.csv >> $OUTFILE_PARAMS
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s5/clusters_macro.csv >> $OUTFILE_MACRO
sed -e '1d' /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s5/clusters_micro.csv >> $OUTFILE_MICRO
