#!/bin/sh
OUTFILE_PARAMS="/cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/params_all.csv"
OUTFILE_MACRO="/cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/clusters_macro_all.csv"
OUTFILE_MICRO="/cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/clusters_micro_all.csv"
cat /cluster/work/scr5/balistef/NEW/attrMillean_nv_rndseq_tm_Rleft/attrMillean_nv_rndseq_tm_Rleft_s0/params.csv > $OUTFILE_PARAMS
