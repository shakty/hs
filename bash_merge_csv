sed -e '1d' /cluster/work/scr4/balistef/NEW/attrExpo_nv_rndseq_tm_Rleft_all_iter/attrExpo_nv_rndseq_tm_Rleft_all_iter_s2/clusters_micro.csv >> $OUTFILE_MICRO
