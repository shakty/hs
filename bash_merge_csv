#!/bin/sh
OUTFILE_PARAMS="/cluster/work/scr4/balistef/NEW/attrExpo_nv_rndseq_tm_Rleft_all_iter/params_all.csv"
OUTFILE_MACRO="/cluster/work/scr4/balistef/NEW/attrExpo_nv_rndseq_tm_Rleft_all_iter/clusters_macro_all.csv"
